<div>
    <form method="post" class="row cl form-horizontal" id="menuForm">
        <input type="hidden" id="menuId" name="menuId">
        <input type="hidden" id="menuParentId" name="menuParentId">
        <input type="hidden" id="menuOrderHid">
        <input type="hidden" id="resourceId">
        <input type="hidden" id="treeTId" name="treeTId">

        <div class="row cl form-element">
            <div class="col-2 form-label">
                标题
            </div>
            <div class="col-10 form-input">
                <input name="menuName" id="menuName" type="text" class="input-text size-M" >
            </div>
        </div>
        <div class="row cl form-element">
            <div class="col-2 form-label">
                排序码
            </div>
            <div class="col-10 form-input">
                <input name="menuOrder" id="menuOrder" type="text" class="input-text size-M" onblur="ztree_check_integer(this,'排序码')">
            </div>
        </div>
        <div class="row cl form-element">
            <div class="col-2 form-label">
                资源名称
            </div>
            <div class="col-10 form-input">
                <input id="resourceName" type="text" class="input-text size-M" disabled="true">
            </div>
        </div>
        <div class="row cl form-element">
            <div class="col-2 form-label">
                资源地址
            </div>
            <div class="col-10 form-input">
                <input id="resourceUrl" type="text" class="input-text size-M" disabled="true">
            </div>
        </div>
        <div class="row cl form-element">
            <div class="col-2 form-label">
                所属业务
            </div>
            <div class="col-10 form-input">
                <input id="businessName" type="text" class="input-text size-M" disabled="true">
            </div>
        </div>
        <div class="row cl pd-10" style="text-align:center;">
            <button class="btn btn-secondary size-M" type="button" id="menuBtn">
                <i class="icon-save icon-large"></i> 保存
            </button>
        </div>
    </form>
</div>